{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "../src",
    "distDir": "../src",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "chess-aa-tauri",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true,
        "execute": true
      },
      "dialog": {
        "all": false,
        "message": true,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "tennis-aa.chess-aa.tauri",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    }
  }
}
